{
    "name": "Fill Form Fields",
    "description": "Extension which helps with filling values into the fields!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "browser_style": false
    },
    "content_scripts":[
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
